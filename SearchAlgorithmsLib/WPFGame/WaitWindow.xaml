<Window x:Class="WPFGame.WaitWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFGame"
        mc:Ignorable="d"
        Title="WaitWindow" Height="300" Width="300">
    <Window.Background>
        <ImageBrush ImageSource="images/wait.png"></ImageBrush>
    </Window.Background>
    <Grid>
        <Button x:Name="BackToMain" Content="Main menu" HorizontalAlignment="Right" VerticalAlignment="Center" Width="95" Margin="0,250,100,0"
                 Click="Back_To_Main_Button_Click" />
    </Grid>
</Window>
